<script id="accessLandlink_PathTmpl" type="text/x-jquery-tmpl">
	
	{{each(i,attribute) $item.attributeList}}


	<tr>
           	<td>
				<a href="javascript:showPathDetails('${attribute.gid}','${attribute.row_id}');">${attribute.row_id}</a>
            </td>
			
			<td>
			${attribute.type}	
			</td>
			<td>
				${attribute._class}	
			</td>
			<td>
			${attribute.lastsurvey}		
				
			</td>	
			<td>
			${attribute.legalstatus}	
			</td>
			
        </tr>
	{{/each}}    
</script>


<script id="accessLandlink_ContactTmpl" type="text/x-jquery-tmpl">
        <tr>
           	<td align="center">
				<a href="javascript:showContactDetails('${contactid}');">${contactid}</a>
            </td>
			<td>
				${firstName}
			</td>
			
			<td>
				${surname}
			</td>
			<td>
				${contactTypeLkp.contactType}
			</td>
			<td>
				${position}
				
			</td>	
			<td>
				${_primary_phone_}
			</td>
			<td>
				${email}
			</td>	
			<td align="center">
				<a href="javascript:deleteAccessLandContact('${contactid}');"><img src="resources/images/studio/delete.png" title="Delete" /></a>
           </td>
        </tr>
</script>
